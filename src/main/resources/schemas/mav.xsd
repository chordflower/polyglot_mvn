        <xs:documentation source="description"><![CDATA[Whether to load Maven extensions (such as packaging and type handlers) from this plugin. For performance reasons, this should only be enabled when necessary.
            Default value is `false`.
          ]]></xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="inherited" type="xs:boolean" default="true">
      <xs:annotation>
        <xs:documentation source="description"><![CDATA[Whether any configuration should be propagated to child POMs. Default value is `true`.]]></xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="PluginExecution">
    <xs:annotation>
      <xs:documentation source="description"><![CDATA[The `execution` element contains informations required for the execution of a plugin.]]></xs:documentation>
