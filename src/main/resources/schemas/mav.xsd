  <xs:complexType name="Resource">
    <xs:annotation>
      <xs:documentation source="description"><![CDATA[This element describes all of the classpath resources associated with a project or unit tests.]]></xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element minOccurs="0" name="includes">
