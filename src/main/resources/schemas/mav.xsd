      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="finalName" type="xs:string">
      <xs:annotation>
        <xs:documentation source="description"><![CDATA[The filename (excluding the extension, and with no path information) that the produced artifact will be called. The default value is `${artifactId}-${version}`.]]></xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
