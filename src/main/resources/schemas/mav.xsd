    </xs:attribute>
    <xs:attribute name="artifactId" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation source="description"><![CDATA[The artifact ID of the plugin in the repository.]]></xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="version" type="xs:string">
      <xs:annotation>
        <xs:documentation source="description"><![CDATA[The version (or valid range of versions) of the plugin to be used.]]></xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="extensions" type="xs:boolean" default="false">
